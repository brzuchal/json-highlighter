{
  "name": "brzuchal/json-highlighter",
  "description": "A PHP library for syntax highlighting JSON in the terminal.",
  "keywords": [
    "json",
    "highlight",
    "colorize",
    "prettify",
    "cli",
    "terminal",
    "console"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Micha≈Ç Marcin Brzuchalski",
      "email": "michal.brzuchalski@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "autoload": {
    "psr-4": {
      "Brzuchal\\JsonHighlighter\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0",
    "doctrine/coding-standard": "^12.0"
  },
  "config": {
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
